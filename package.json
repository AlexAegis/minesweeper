{
	"name": "svelte-minesweeper",
	"displayName": "Svelte Minesweeper",
	"description": "Svelte Minesweeper",
	"version": "1.0.0",
	"license": "mit",
	"private": true,
	"author": {
		"name": "Alex Aegis",
		"email": "alexaegis@gmail.com",
		"url": "https://github.com/AlexAegis"
	},
	"homepage": "https://github.com/AlexAegis/svelte-minesweeper",
	"bugs": {
		"email": "alexaegis@gmail.com",
		"url": "https://github.com/AlexAegis/svelte-minesweeper/issues"
	},
	"scripts": {
		"build": "rollup -c",
		"roll": "rollup -c -w",
		"serve": "live-server public --port=8989",
		"start": "concurrently --kill-others --timestamp-format \"HH:mm:ss.SSS\" --prefix-colors \"bgRed.black.dim,bgCyan.black.dim\" --prefix \"{name} {time} {pid}\" --names \"ROLL,HTTP\" \"npm run roll\" \"npm run serve\"",
		"lint": "concurrently --timestamp-format \"HH:mm:ss.SSS\" --prefix-colors \"bgYellow.black.dim,bgCyan.black.dim\" --prefix \"{name} {time} {pid}\" --names \"SVELTE,ESLINT\" \"svelte-check\" \"eslint .\"",
		"test": "jest --passWithNoTests",
		"lint:css": "stylelint --allow-empty-input --syntax=css ./**/*.css",
		"lint:scss": "stylelint --allow-empty-input --syntax=scss ./**/*.scss",
		"lint:md": "markdownlint --ignore 'CHANGELOG.md' --ignore-path '.gitignore' .",
		"lint:rmd": "remark .",
		"lint:format": "prettier --check .",
		"nuke": "rimraf ./node_modules ./package-lock.json",
		"ci": "concurrently --timestamp-format \"HH:mm:ss.SSS\" --prefix-colors \"bgRed.black.dim,bgCyan.black.dim\" --prefix \"{name} {time} {pid}\" --names \"LINT,LINT:CSS,LINT:SCSS,LINT:MD,LINT:RMD,LINT:FORMAT,TEST,BUILD\" \"npm run lint\" \"npm run lint:css\" \"npm run lint:scss\" \"npm run lint:md\" \"npm run lint:rmd\" \"npm run lint:format\" \"npm run test\" \"npm run build\"",
		"ncu": "ncu -u",
		"format": "prettier --write .",
		"postinstall": "husky install",
		"prepublishOnly": "pinst --disable && npm run build",
		"postpublish": "pinst --enable",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d public"
	},
	"dependencies": {
		"rxjs": "^7.5.6",
		"svelte": "^3.50.1",
		"svelte-observable": "^0.4.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@rollup/plugin-commonjs": "^22.0.2",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^14.0.1",
		"@rollup/plugin-typescript": "^8.5.0",
		"@tsconfig/svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
		"concurrently": "^7.4.0",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"gh-pages": "^4.0.0",
		"husky": "^8.0.1",
		"jest": "^29.0.2",
		"lint-staged": "^13.0.3",
		"live-server": "^1.1.0",
		"markdownlint": "^0.26.2",
		"markdownlint-cli": "^0.32.2",
		"npm-check-updates": "^16.1.1",
		"nyc": "^15.1.0",
		"prettier": "^2.7.1",
		"remark": "^14.0.2",
		"remark-cli": "^11.0.0",
		"remark-lint": "^9.1.1",
		"remark-lint-maximum-line-length": "^3.1.2",
		"remark-preset-lint-consistent": "^5.1.1",
		"remark-preset-lint-recommended": "^6.1.2",
		"rollup": "^2.79.0",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-gzip": "^3.0.1",
		"rollup-plugin-livereload": "^2.0.5",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"stylelint": "^14.11.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-prettier": "^2.0.0",
		"svelte-check": "^2.9.0",
		"svelte-preprocess": "^4.10.7",
		"terser": "^5.15.0",
		"ts-jest": "^29.0.0",
		"tslib": "^2.4.0",
		"typescript": "^4.8.3"
	}
}
